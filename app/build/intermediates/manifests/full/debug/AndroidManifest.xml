<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.discover.yahoohack"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_discover"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.discover.yahoohack.MainActivity"
            android:icon="@drawable/logo_discover"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.discover.yahoohack.ResultActivity"
            android:icon="@drawable/logo_discover"
            android:label="Results" >
        </activity>
        <activity
            android:name="com.discover.yahoohack.WebviewActivity"
            android:icon="@drawable/logo_discover"
            android:label="Discover24" >
        </activity>
    </application>

</manifest>